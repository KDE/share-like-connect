<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="org.kde.ActiveContent">
        <method name="activeContent">
            <arg name="url" type="s" direction="out">
            <arg name="title" type="s" direction="out">
            <arg name="mimetype" type="s" direction="out">
            <arg name="serviceIdentifier" type="s" direction="out">
            <arg name="windowId" type="i" direction="out">
        </method>
        <signalname="activeContentChanged">
            <arg name="url" type="s" direction="out">
            <arg name="title" type="s" direction="out">
            <arg name="mimetype" type="s" direction="out">
            <arg name="serviceIdentifier" type="s" direction="out">
            <arg name="windowId" type="i" direction="out">
        </method>
    </interface>
</node>
